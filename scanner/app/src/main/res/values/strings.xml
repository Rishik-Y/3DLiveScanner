    <string name="poisson">Poisson reconstruction</string>
    <string name="poisson_summary">Enable postprocessing algorithm to fix holes in model geometry, note that this algorithm does not work well with open spaces and it can make postprocessing longer.</string>
    <string name="textures">Limit texture count</string>
    <string name="textures_summary">Limits the amount of textures. More textures provides better visual quality.</string>
    <string-array name="textures_names">
        <item>Single texture</item>
        <item>Max 4 textures</item>
        <item>Do not limit</item>
    </string-array>
    <string name="anchor">Show anchors during scanning</string>
    <string name="anchor_summary">Anchors are used to stabilize tracking of the device position.</string>
    <string name="grid">Show floor grid</string>
    <string name="grid_summary">Grid makes it easier to understand the 3D space.</string>
    <string name="camera">Show camera data during scanning</string>
    <string name="camera_summary">Showing camera produces additional information about the scanning process, however it could cause device overheating and lower scanning performance.</string>
    <string name="offset">Add offset to the geometry</string>
    <string name="offset_summary">Adding offset to the geometry enables to scan thin objects from both sides. This option makes the geometry less accurate.</string>
    <string name="depth_sensor">Use depth sensor</string>
    <string name="depth_sensor_summary">Active depth sensor is more accurate and scans more details.</string>
    <string name="gps">Use GPS to tag the position</string>
    <string name="gps_summary">Using a GPS tag is possible to show on the map where the model was captured.</string>
    <string name="fullhd">Capture fullHD photos</string>
    <string name="fullhd_summary">Capturing fullHD photos into dataset enables you to use the data in photogrammetry software. Note that this option slows down the scanning process.</string>
    <string name="align_pose">Align device pose</string>
    <string name="align_pose_summary">Adjusts device pose to increase the texture quality. This option makes capturing much slower.</string>
    <string name="flash_light">Use flashlight</string>
    <string name="flash_light_summary">Flashlight makes easier scanning in dark places.</string>
    <string name="decimation">Decrease polygon count</string>
    <string name="decimation_summary">As a part of postprocessing there is a process called decimation which decreases amount of polygons.</string>
    <string-array name="decimation_names">
        <item>Maximal detail</item>
        <item>Optimal for VR</item>
        <item>Low polygon count</item>
    </string-array>
    <string name="limit">Limit scanning distance</string>
    <string name="limit_summary">By limiting scanning distance it is easier to scan only part of the object and filter far depth data with lower accuracy.</string>
    <string-array name="limit_names">
        <item>1 meter - objects only</item>
        <item>2 meters - best precision</item>
        <item>4 meters - fast scanning</item>
        <item>No software limit</item>
    </string-array>
    <string name="noise">Noise filter</string>
    <string name="noise_summary">Using noise filter reduces amount of wrong geometry in the scan. Note that it decreases the speed of scanning.</string>
    <string-array name="noise_names">
        <item>No noise filter</item>
        <item>Normal noise filter</item>
        <item>Aggressive noise filter</item>
    </string-array>
    <string name="clearing">Space clearing</string>
    <string name="clearing_summary">This feature clears data from scene which were only temporarily visible.</string>

